<script type="text/javascript">
    var pagesql=1;
    function maslistadoUN(){
      $("#vermasboton").hide();
      pagesql= pagesql+1;
      $.post("/a/aps/noticias/includes/ajax/listadoUN.asp",{page: pagesql},function(data){
        if(data!="")
        {
          $(".ultimoInf").append(data);
          $("#vermasboton").show();
        }else
        {
          $("#vermasboton").hide();
        }
      });
   }
  
</script>
     


